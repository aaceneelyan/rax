<div id="fws_65c9ece1044df" data-column-margin="default" data-midnight="light" class="wpb_row vc_row-fluid vc_row full-width-content vc_row-o-equal-height vc_row-flex vc_row-o-content-top  right_padding_8vw left_padding_8vw top_padding_tablet_100px top_padding_phone_50px bottom_padding_tablet_100px bottom_padding_phone_50px right_padding_tablet_75px right_padding_phone_50px left_padding_tablet_75px left_padding_phone_50px" style="padding-top: 125px; padding-bottom: 125px; "><div class="row-bg-wrap" data-bg-animation="none" data-bg-animation-delay data-bg-overlay="false"><div class="inner-wrap row-bg-layer"><div class="row-bg viewport-desktop using-bg-color" style="background-color: #000000; "></div></div></div><div class="row_col_wrap_12 col span_12 light left">
<div class="vc_col-sm-12 wpb_column column_container vc_column_container col no-extra-padding el_spacing_0px inherit_tablet inherit_phone border_color_474747 border_style_solid " data-padding-pos="all" data-has-bg-color="false" data-bg-color data-bg-opacity="1" data-animation data-delay="0">
